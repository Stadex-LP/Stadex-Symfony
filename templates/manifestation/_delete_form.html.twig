<form style="display: inline;" method="post" action="{{ path('app_manifestation_delete', {'id': manifestation.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette Manifestation ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ manifestation.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
